}
}
if (cr * cb * cw == 0) {
# not all colors in the current experiment
cntr = cntr + 1
}
}
# Week 06: Discrete Probability I - Exercise II
# execute the experiment 50000 times
n    = 1000000
# initialize counter for cases with at least one missing color
cntr = 0
for (i in c(1:n)) {
# initialize counters for red, blue and white balls
cr = 0
cb = 0
cw = 0
# generate 10 random number between 1 and 90
r <- sample(1:90,10)
for (j in c(1:10)) {
if (r[j] >= 1 & r[j] <= 30) {
# random numbers from  1 to 30 represent red balls: increase counter for red balls
cr = cr + 1
} else if (r[j] >=31 & r[j] <= 60) {
# random numbers from 31 to 60 represent blue balls: increase counter for blue balls
cb = cb + 1
} else if (r[j] >=61 & r[j] <= 90) {
# random numbers from 61 to 90 represent blue balls: increase counter for white balls
cw = cw + 1
} else {
stop("values must ly between 1 and 90")
}
}
if (cr * cb * cw == 0) {
# not all colors in the current experiment
cntr = cntr + 1
}
}
# probability for at least one missing color
p = cntr/n
p
